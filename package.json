    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.8.4",
    "concurrently": "^9.0.1",
    "cross-env": "^7.0.3",
    "deepl-node": "^1.14.0",
    "dotenv-cli": "^7.4.2",
    "exceljs": "^4.4.0",
    "jest": "^29.7.0",
    "pino-pretty": "^11.3.0",
    "prisma": "^5.21.1",
    "supertest": "^7.0.0",
    "typescript": "^5.6.3"
  },
