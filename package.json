    "@prisma/client": "^5.21.1",
    "@types/node": "^20.12.12",
    "bullmq": "^5.21.2",
    "chromadb": "^1.9.2",
    "compromise": "^14.13.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "jest": "^29.7.0",
    "openai": "^4.68.4",
    "pdf-to-img": "^4.1.1",
    "pino-http": "^10.3.0",
    "prisma-zod-generator": "^0.8.13",
